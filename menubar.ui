<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkMenuBar" id="menubar">
    
    <child>  
      <object class="GtkMenuItem" id="file">
        <property name="label">File</property>
        <child type="submenu">
          <object class="GtkMenu" id="file_menu">
            
            <child>
              <object class="GtkMenuItem" id="file_open">
                <property name="label">Open</property>
              </object>
            </child>
            
            <child>
              <object class="GtkMenuItem" id="file_exit">
                <property name="label">Exit</property>
              </object>
            </child>
            
          </object>
        </child>
      </object>
    </child>
    
    <child>
      <object class="GtkMenuItem" id="edit">
        <property name="label">Edit</property>
        <child type="submenu">
          <object class="GtkMenu" id="edit_menu">
            
            <child>
              <object class="GtkMenuItem" id="edit_copy">
                <property name="label">Copy</property>
              </object>
            </child>
            
            <child>
              <object class="GtkMenuItem" id="edit_cut">
                <property name="label">Cut</property>
              </object>
            </child>
            
          </object>
        </child>
      </object>
    </child>
    
  </object>
</interface>
